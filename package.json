{
  "name": "ui",
  "version": "0.0.0",
  "scripts": {
    "dev": "NODE_ENV=development next dev --hostname 127.0.0.1",
    "build": "next build",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "lint:fix": "eslint . --fix --ext ts --ext tsx --ext js",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "start": "next start",
    "test": "env TZ=Etc/UTC jest --runInBand",
    "test:watch": "env TZ=Etc/UTC jest --watch  --runInBand"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@next/bundle-analyzer": "13.4.20-canary.18",
    "next": "13.5.4",
    "next-secure-headers": "2.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "undici": "^5.23.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "13.4.20-canary.18",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.4",
    "@types/node": "^18.17.14",
    "@types/react": "^18.2.21",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "jest-environment-node": "^29.6.4",
    "jest-junit": "^16.0.0",
    "jest-serializer-path": "^0.1.15",
    "jest-snapshot-serializer-raw": "1.2.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "typescript": "^4.9.5"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
